{% extends 'base.html.twig' %}


{% block body %}
	{{ form_start(form) }}
	<main>
		<div class="nameRegister">
			<h1>Inscription</h1>
		</div>
		<div class="register-container">
			{{ form_row(form.firstName) }}
			{{ form_row(form.lastName) }}
			{{ form_row(form.email) }}
			{{ form_row(form.password) }}
			{{ form_row(form.submit) }}
		</div>
	</main>
	{{ form_end(form) }}
	{% block footer %}{% endblock %}
{% endblock %}
